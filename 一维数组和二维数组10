#include<iostream.h>
void main( )
{
    int b1[3][4],b2[3][4],i,j,k=1;
    for(i=0;i<3;i++)
        for(j=0;j<4;j++)
            bi[i][j]=k++;
        for(i=0;i<3;i++)
            for(j=0;j<4;j++)
            {
                b2[i][0]=b1[i][3];
                b2[i][3]=b1[i][2];
                b2[i][1]=b1[i][0];
                b2[i][2]=b1[i][1];
            }
cout<<"原数组为:\n";
for(i=0;i<3;i++){
    for(j=0;j<4;j++)cout<b1[i][j]<<'\t';
    cout<<'\n';
}
cout<<"转置后的数组为:\n";
for(i=0;i<3;i++){
    for(j=0;j<4;j++)cout<<b2[i][j]<<'\t';
    cout<<'\n';
