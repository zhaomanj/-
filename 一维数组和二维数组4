/* 第2关：查找整数 */

#include<stdio.h>

#define N 1000
int a[N];

int main(void)
{
    /*********Begin*********/
    int n, x, i;

    /* 读入数据 */
    scanf("%d", &n);
    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);
    scanf("%d", &x);
    
    /* 查找数据 */
    for(i = 0; i < n; i++)
        if(a[i] == x) break;

    printf("%d\n", i == n ? -1 : i + 1);    
    /*********End**********/
    return 0;
}
