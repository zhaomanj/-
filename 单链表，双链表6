class Solution:
    def search(self, nums: List[int], target: int) -> int:
        left = 0
        right = len(nums)-1
        while left<=right:
            mid = left+(right-left)//2
            print(mid, left, right)
            if nums[mid] == target:
                return mid
            # 【mid,right】连续增长
            elif nums[mid] <= nums[right]:
                if nums[mid]<target<=nums[right]:
                    left = mid+1
                else:
                    right = mid-1
            else:
                if nums[left]<=target<nums[mid]:
                    right = mid-1
                else:
                    left = mid+1
         
        return -1
       
