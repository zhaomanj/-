#define _CRT_SECURE_NO_WARNINGS

#include<stdio.h>
#include<string.h>

int main() {
  
  char str1[30], str2[30];
  printf("请输入目的字符串：\n");
  gets_s(str1, 30);
  printf("请输入源字符串：\n");
  gets_s(str2, 30);

  printf("输出目的字符串：\n");
  puts(str1);
  printf("输出源字符串：\n");
  puts(str2);

  strcat(str1, str2);
  printf("调用strcat函数后目的字符串：\n");
  puts(str1);
  return 0;
}
